#: src/trip_get_options.cpp:118
msgid "configuration file name"
msgstr "nom de fichier de configuration"

#. Command line short description for the --upgrade option
#: src/trip_get_options.cpp:120
msgid "upgrade database"
msgstr "optimise la base de données"

#. Command line short description for the --verbose option
#: src/trip_get_options.cpp:122
msgid "verbose output"
msgstr "output verbeux"

